[  {    "type": "post",    "url": "/accounts/:account_id/policy_keys",    "title": "Create a Policy Key",    "name": "Create_a_Policy_Key",    "group": "Policy",    "version": "1.0.0",    "description": "<p>Create a new policy key to access the Playback API</p>",    "header": {      "fields": {        "Header": [          {            "group": "Header",            "type": "String",            "optional": false,            "field": "Content-Type",            "description": "<p>Content-Type: application/json</p>"          },          {            "group": "Header",            "type": "String",            "optional": false,            "field": "Authorization",            "description": "<p>Authorization: Bearer access_token (see <a href=\"http://docs.brightcove.com/en/video-cloud/oauth-api/guides/get-token.html\">Getting Access Tokens</a>)</p>"          }        ]      }    },    "parameter": {      "fields": {        "Path Parameters": [          {            "group": "Path Parameters",            "type": "String",            "optional": false,            "field": "account_id",            "description": "<p>Video Cloud account ID</p>"          }        ],        "Request Body Fields (Concise Format)": [          {            "group": "Request Body Fields (Concise Format)",            "type": "Object",            "optional": false,            "field": "key-data",            "description": "<p>data for the policy key (used for concise format)</p>"          },          {            "group": "Request Body Fields (Concise Format)",            "type": "String",            "optional": false,            "field": "key-data.account-id",            "description": "<p>Video Cloud account id</p>"          },          {            "group": "Request Body Fields (Concise Format)",            "type": "String[]",            "optional": true,            "field": "key-data.allowed-domains",            "description": "<p>For doma